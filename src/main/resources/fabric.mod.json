  "authors": [
    "LCLP"
  ],
  "contact": {
    "homepage": "https://github.com/LCLPYT/TheFabricArchitect",
    "sources": "https://github.com/LCLPYT/TheFabricArchitect"
  },

  "license": "MIT",
  "icon": "assets/mightyarchitect/icon.png",

